        <?php 
            echo "let's start!!!<br>";
            echo "<hr>";
            $end = '';
            if(is_float($end))
            {
                echo "end is float<br>";
            }else {
                echo "end is not a float<br>";
            }
            echo "<hr>";
            
           //测试$end 是否为空
           if(empty($end))
           {
               echo "end is empty<br>";
           }else {
               echo "end is not empty<br>";
           }
           echo "<hr>";
           
           //对数组的进行测试
           $_info = array();
           $_start = 'hello';
           $_info[$start] = 12345;
           echo "$_info[$start]<br>";
           echo "<hr>";
        ?>
