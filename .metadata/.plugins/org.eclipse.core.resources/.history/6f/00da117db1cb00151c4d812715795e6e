            $end = '';
            if(is_float($end))
            {
                echo "end is float<br>";
            }else {
                echo "end is not a float<br>";
            }
            echo "<hr>";
            
           //测试$end 是否为空
           if(empty($end))
           {
               echo "end is empty<br>";
           }else {
               echo "end is not empty<br>";
           }
           echo "<hr>";
           
           //对数组的进行测试
           $_info = array();
           $_start = 'hello';
           $_info[$_start] = 12345;
           echo "$_info[$_start]<br>";
           echo "<hr>";
           //通过对数组的测试，发现数组的下表可以是字符串，
           //只要第二次给出相应的下表，则可以取出字符串中的内容
           
           //测试isset函数
           echo "测试isset函数<br>";
           $temp = array();
           $temp[0]="hello world";
           echo "temp[0]<br>";
           if(isset($temp[0]))
           {
               echo "数组中存在数据<br>";
           }else {
               echo "数组中不存在数据<br>";
           }
           echo "temp[1]<br>";
           if(isset($temp[1]))
           {
               echo "数组中存在数据<br>";
           }else {
               echo "数组中不存在数据<br>";
           }
           echo "<hr>";
           
           echo "<hr>";
           echo "对文件作出判断";
